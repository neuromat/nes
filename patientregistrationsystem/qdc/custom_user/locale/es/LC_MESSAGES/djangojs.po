#: custom_user/forms.py:19
msgid "Type first name"
msgstr "Ingrese su nombre"

#: custom_user/forms.py:20
msgid "Type last name"
msgstr "Ingrese su apellido"

#: custom_user/forms.py:22
msgid "Type user name"
msgstr "Ingrese su nombre de usuario"

#: custom_user/forms.py:22
msgid "Type citation name"
msgstr "Ingrese el nombre para la citación"

#: custom_user/forms.py:24 custom_user/forms.py:47
msgid "Type password"
msgstr "Ingrese su contraseña"

#: custom_user/forms.py:27
msgid "Type e-mail"
msgstr "Ingrese su correo"

#: custom_user/forms.py:73 custom_user/forms.py:74
msgid "E-mail is not registered"
msgstr "El correo electrónico no está registrado"

#: custom_user/templates/custom_user/register_users.html:34
msgid ""
"Password must contain at least 8 characters, including at least one "
"uppercase letter, digit or special character."
msgstr ""
"La contraseña debe contener al menos 8 caracteres, incluido al menos una "
"letra mayúscula, dígito o carácter especial."

#: custom_user/templates/custom_user/register_users.html:42
msgid "You have to choose at least one user profile!"
msgstr "Elija al menos un perfil de usuario"

#: custom_user/templates/custom_user/register_users.html:67
msgid "E-mail have to be filled!"
msgstr "El correo electrónico debe ser ingresado"

#: custom_user/templates/custom_user/register_users.html:109
msgid "Please, correct the fields highlighted."
msgstr "Por favor, corrija los campos resaltados."

#: custom_user/tests.py:84
